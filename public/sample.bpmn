<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_1"
                  targetNamespace="http://bpmn.io/schema/bpmn">

  <bpmn:process id="Process_OrderFulfillment" name="Order Fulfillment Process" isExecutable="true">

    <bpmn:startEvent id="StartEvent_OrderReceived" name="Order Received">
      <bpmn:documentation>Customer places an order through the website or mobile app.</bpmn:documentation>
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:task id="Task_ValidateOrder" name="Validate Order">
      <bpmn:documentation>Check order details, customer information, and payment status.</bpmn:documentation>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>

    <bpmn:exclusiveGateway id="Gateway_OrderValid" name="Order Valid?">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:task id="Task_CheckInventory" name="Check Inventory">
      <bpmn:documentation>Verify product availability in warehouse.</bpmn:documentation>
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_RejectOrder" name="Reject Order">
      <bpmn:documentation>Send rejection notification to customer with reason.</bpmn:documentation>
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_10</bpmn:outgoing>
    </bpmn:task>

    <bpmn:exclusiveGateway id="Gateway_InStock" name="In Stock?">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:task id="Task_PackOrder" name="Pack Order">
      <bpmn:documentation>Pick items from warehouse and prepare package for shipping.</bpmn:documentation>
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_8</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_BackOrder" name="Create Back Order">
      <bpmn:documentation>Place item on back order and notify customer of delay.</bpmn:documentation>
      <bpmn:incoming>Flow_7</bpmn:incoming>
      <bpmn:outgoing>Flow_11</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_ShipOrder" name="Ship Order">
      <bpmn:documentation>Hand over package to shipping carrier and send tracking info.</bpmn:documentation>
      <bpmn:incoming>Flow_8</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:task>

    <bpmn:endEvent id="EndEvent_OrderComplete" name="Order Completed">
      <bpmn:incoming>Flow_9</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:endEvent id="EndEvent_OrderRejected" name="Order Rejected">
      <bpmn:incoming>Flow_10</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:endEvent id="EndEvent_BackOrdered" name="Back Ordered">
      <bpmn:incoming>Flow_11</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_OrderReceived" targetRef="Task_ValidateOrder" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_ValidateOrder" targetRef="Gateway_OrderValid" />
    <bpmn:sequenceFlow id="Flow_3" name="Yes" sourceRef="Gateway_OrderValid" targetRef="Task_CheckInventory" />
    <bpmn:sequenceFlow id="Flow_4" name="No" sourceRef="Gateway_OrderValid" targetRef="Task_RejectOrder" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_CheckInventory" targetRef="Gateway_InStock" />
    <bpmn:sequenceFlow id="Flow_6" name="Yes" sourceRef="Gateway_InStock" targetRef="Task_PackOrder" />
    <bpmn:sequenceFlow id="Flow_7" name="No" sourceRef="Gateway_InStock" targetRef="Task_BackOrder" />
    <bpmn:sequenceFlow id="Flow_8" sourceRef="Task_PackOrder" targetRef="Task_ShipOrder" />
    <bpmn:sequenceFlow id="Flow_9" sourceRef="Task_ShipOrder" targetRef="EndEvent_OrderComplete" />
    <bpmn:sequenceFlow id="Flow_10" sourceRef="Task_RejectOrder" targetRef="EndEvent_OrderRejected" />
    <bpmn:sequenceFlow id="Flow_11" sourceRef="Task_BackOrder" targetRef="EndEvent_BackOrdered" />

  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Process_OrderFulfillment">

      <bpmndi:BPMNShape id="StartEvent_OrderReceived_di" bpmnElement="StartEvent_OrderReceived">
